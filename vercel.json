{
  "headers": [
    {
      "source": "/maps/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://mappoolgenerator.shop https://map-pool-generator.vercel.app"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; img-src https://mappoolgenerator.shop https://map-pool-generator.vercel.app;"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}